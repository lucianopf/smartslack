{
  "name": "smartslack",
  "version": "0.0.1",
  "description": "A simple Slack API client for node.js",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philliphenslee/smartslack.git"
  },
  "keywords": [
    "bot",
    "javascript",
    "node",
    "slack"
  ],
  "author": "Phillip Henslee <ph2@ph2.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philliphenslee/smartslack/issues"
  },
  "homepage": "https://github.com/philliphenslee/smartslack#readme",
  "dependencies": {
    "bistre": "^1.0.1",
    "bole": "^2.0.0",
    "lodash": "^3.10.1",
    "ws": "^0.8.0"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "coveralls": "^2.11.4",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3",
    "nock": "^2.15.0",
    "sinon": "^1.17.1"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -R spec test/*.js",
    "test-travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*",
    "travis": "npm test"
  },
  "config": {
    "blanket": {
      "pattern": "test",
      "data-cover-flags": {
        "debug": false
      }
    }
  }
}
